# This is an example environment file for the Grafana Mobile Alerts project.
# Rename this file to `.env` and adjust the values as needed.

# Database credentials
DB_HOST=timescaledb
DB_PORT=5432
DB_NAME=sensor_data
DB_USER=sensor_user
DB_PASSWORD=sensor_password

# Fetcher configuration
API_URL=https://www.data199.com/api/pv1/device/lastmeasurement
SENSOR_IDS=sensor1,sensor2,sensor3
FETCH_INTERVAL_SECONDS=300

# Grafana admin credentials and locale
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=admin
